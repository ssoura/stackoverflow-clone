{
  "name": "stackoverflow-lite",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*"
  
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --no-cache --parallel --continue",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\" "
  },
  "devDependencies": {
    "eslint": "^8.11.0",
    "prettier": "^2.6.0",
    "turbo": "^1.1.6"
  }
}
